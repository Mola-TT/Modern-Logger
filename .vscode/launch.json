{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Run Console Logger Example",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/example_usage.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Debug Console Logger Example",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/example_usage.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "stopOnEntry": false,
            "showReturnValue": true
        }
    ],
    "compounds": []
}
